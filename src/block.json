{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wcrh/custom-archive",
	"version": "0.1.0",
	"title": "Archive",
	"category": "custom-block",
	"attributes": {
		"posts": {
			"type": "array",
			"default": []
		},
		"posttype": {
			"type": "string"
		},
		"numPosts": {
			"type": "string",
			"default": 4
		},
		"color": {
			"type": "string"
		}
	},
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "custom-archive",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
